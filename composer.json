{
    "name": "yybawang/laravel-ebank",
    "type": "library",
    "description": "A library for unified payments with online wallets and Alipay and WeChat",
    "keywords": ["ebank", "wallets", "online-wallets", "wechat-pay", "alipay", "unified-payments"],
    "homepage": "https://symfony.com",
    "license": "MIT",
    "version": "1.0.0",
    "authors": [
        {
            "name": "yybawang",
            "email": "yybawang@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "yansongda/pay": "^2.9.0"
    },
    "autoload": {
        "psr-4": {
            "yybawang\\ebank\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "yybawang\\ebank\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "yybawang\\ebank\\ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true
}
